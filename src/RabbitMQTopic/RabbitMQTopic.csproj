<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Jerry Bai</Authors>
    <PackageProjectUrl>https://github.com/berkaroad/RabbitMQTopic</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>RabbitMQTopic</PackageId>
    <Title>RabbitMQTopic</Title>
    <Version>1.2.1</Version>
    <AssemblyVersion>1.2.0</AssemblyVersion>
    <PackageTags>rabbitmq mq rocketmq topic</PackageTags>
    <Description>模拟RocketMQ的Topic方式的消息发送和接收。</Description>
    <PackageReleaseNotes>1）优化Pull模式，在空转情况下，平衡实时性和空转性能消耗。
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="5.0.1" />
  </ItemGroup>

</Project>
